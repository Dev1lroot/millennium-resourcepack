{
  "model": {
    "type": "minecraft:condition",
    "model": "minecraft:item/bread",
    "fallback": {
      "type": "model",
      "model": "minecraft:item/bread"
    },
    "property": "minecraft:has_component",
    "component": "minecraft:custom_name",
    "on_true": {
      "type": "minecraft:select",
      "model": "minecraft:item/bread",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when": ["Croissant","Круассан"],
          "model": {
            "type": "model",
            "model": "minecraft:item/food/croissant"
          }
        },
        {
          "when": ["Baguette","Багет"],
          "model": {
            "type": "model",
            "model": "minecraft:item/food/baguette"
          }
        },
        {
          "when": ["Toffee","Ириска"],
          "model": {
            "type": "model",
            "model": "minecraft:item/food/toffee"
          }
        }
      ],
      "fallback": {
        "type": "model",
        "model": "minecraft:item/bread"
      }
    },
    "on_false": {
      "type": "model",
      "model": "minecraft:item/bread"
    }
  }
}